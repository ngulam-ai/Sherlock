<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app
	xmlns="http://appengine.google.com/ns/1.0">
	<application>${appengine.app.name}</application>
	<!-- We do not specify the module name so this is the default module. There 
		is only 1 default module per application. -->
	<!-- <module>default</module> -->
	<version>${appengine.app.version}</version>

	<threadsafe>true</threadsafe>

	<sessions-enabled>false</sessions-enabled>
	<runtime>java8</runtime>

	<instance-class>F1</instance-class>

	<automatic-scaling>
		<!-- min-idle-instances>2</min-idle-instances> <max-pending-latency>1s</max-pending-latency> 
			<min-pending-latency>1s</min-pending-latency -->
	</automatic-scaling>

</appengine-web-app>