<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:m="urn:import:agency.akcom.mmg.sherlock.ui.client.widget" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:with field="nameTokens" type="agency.akcom.mmg.sherlock.ui.client.place.NameTokens" />
		<g:ScrollPanel>
			<g:HTMLPanel>

				<b:Navbar position="FIXED_TOP">

					<b:NavbarHeader>
						<b:NavbarBrand targetHistoryToken="{nameTokens.getHome}"
							ui:field="brand">SHERLOCK
						</b:NavbarBrand>
						<b:NavbarCollapseButton dataTarget="#navbar-collapse" />
					</b:NavbarHeader>
					<b:NavbarCollapse b:id="navbar-collapse">
						<b:NavbarNav pull="RIGHT">
							
							<b:AnchorListItem text="Import log"
								targetHistoryToken="{nameTokens.getLog}" />
							<b:AnchorListItem text="Users"
								targetHistoryToken="{nameTokens.getUsers}" />
							<b:ListDropDown>
								<m:UserDropDownToggle ui:field="userDdAnchor" />
								<b:DropDownMenu>
									<b:AnchorListItem text="Sign Out" ui:field="signOut" />
								</b:DropDownMenu>
							</b:ListDropDown>
						</b:NavbarNav>
					</b:NavbarCollapse>

				</b:Navbar>


				<!-- Container Content -->
				<g:SimplePanel ui:field="main" />



			</g:HTMLPanel>
		</g:ScrollPanel>
</ui:UiBinder>