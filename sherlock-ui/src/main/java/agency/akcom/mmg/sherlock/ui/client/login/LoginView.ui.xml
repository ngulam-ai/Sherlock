<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>



	<g:ScrollPanel>
		<g:HTMLPanel>
			<b:Container>
				<b:Row>
					<b:Column size="XS_12">
						<b:PageHeader text="SHERLOCK web application" subText="" />
						<b:Panel>
							<b:PanelHeader>
								<b:Heading size="H3" text="Login" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="XS_12">
										<b:Button text="Sign in with Google"
											ui:field="signInWithGoogle" />
									</b:Column>
								</b:Row>
							</b:PanelBody>
						</b:Panel>
					</b:Column>
				</b:Row>

			</b:Container>


		</g:HTMLPanel>
	</g:ScrollPanel>
</ui:UiBinder>